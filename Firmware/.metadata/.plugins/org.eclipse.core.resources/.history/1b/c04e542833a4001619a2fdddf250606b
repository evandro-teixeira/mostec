#include "main.h"

int main(void)
{
	int counter = 0;
	rgb_init();
	
	put("\r\nHello World! UART Lab using Interrupt mode\r\n");
	
	EnableInterrupts;
	enable_irq(INT_UART0 -16);
	set_irq_priority((INT_UART0 -16), 2);
	
	while(TRUE)
	{
		counter++;
		if(counter >= DLY)
		{
			GREEN_TOGGLE;
			counter = 0;
		}
	}
	
	return 0;
}
